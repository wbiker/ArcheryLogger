% layout 'default';
% title 'Welcome';
<% my $sessions = stash("sessions"); %>

<table>
<tr>
<th>Date</th>
<th>Score Per Hit Target</th>
</tr>
% for my $session (@{$sessions}) {
<tr>
<td><%= $session->{date} %></td>
<td><%= $session->{score_per_hit_targets} %></td>
</tr>
% }
</table>

<img alt="score per hit target chart" src="data:image/png;base64,<%= $image %>" />
